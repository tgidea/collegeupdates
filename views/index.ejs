  <script>
    const setHead = function () {
      const infobox = document.getElementById('infobox');
      const dummy = document.getElementById('dummy');
      const colour = infobox.innerText.toString();
      const val = dummy.innerText;
      infobox.innerHTML = `${val}`;
      infobox.style.backgroundColor = `${colour}`;
    }
    setHead();
    const submit = document.getElementById('submit');
    setTimeout(function () {
      submit.innerText = "";

    }, 5000);
  </script>
